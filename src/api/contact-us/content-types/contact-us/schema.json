{
  "kind": "collectionType",
  "collectionName": "contact_uses",
  "info": {
    "singularName": "contact-us",
    "pluralName": "contact-uses",
    "displayName": "Contact Us",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "read",
        "unread"
      ],
      "required": true,
      "default": "unread"
    }
  }
}
